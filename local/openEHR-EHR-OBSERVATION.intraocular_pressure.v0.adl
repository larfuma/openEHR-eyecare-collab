definition
	OBSERVATION[at0000] matches {    -- Intraocular pressure
		data matches {
			HISTORY[at0001] matches {    -- Event Series
				events cardinality matches {1..*; unordered} matches {
					EVENT[at0002] occurrences matches {0..*} matches {    -- Any event
						data matches {
							ITEM_TREE[at0003] matches {    -- Tree
								items cardinality matches {0..*; unordered} matches {
									ELEMENT[at0057] matches {    -- Eye examined
										value matches {
											DV_CODED_TEXT matches {
												defining_code matches {
													[local::
													at0058,    -- Left eye
													at0059]    -- Right eye
												}
											}
										}
									}
									ELEMENT[at0099] occurrences matches {0..1} matches {    -- Palpatory estimation
										value matches {
											DV_CODED_TEXT matches {
												defining_code matches {
													[local::
													at0100,    -- No pressure
													at0101,    -- softer than normal
													at0102,    -- approximately normal
													at0103,    -- harder than normal
													at0104]    -- very hard
												}
											}
										}
									}
									ELEMENT[at0042] occurrences matches {0..1} matches {    -- Pressure
										value matches {
											C_DV_QUANTITY <
												property = <[openehr::125]>
												list = <
													["1"] = <
														units = <"mm[Hg]">
														magnitude = <|0.0..20000.0|>
														precision = <|2|>
													>
												>
											>
										}
									}
									CLUSTER[at0092] occurrences matches {0..*} matches {    -- Intraocular pressure correction details
										items cardinality matches {1..*; unordered} matches {
											allow_archetype CLUSTER[at0091] occurrences matches {0..1} matches {    -- Central corneal thickness details
												include
													archetype_id/value matches {/openEHR-EHR-CLUSTER\.corneal_thickness_details\.v0/}
											}
											ELEMENT[at0081] occurrences matches {0..1} matches {    -- Corrected pressure
												value matches {
													C_DV_QUANTITY <
														property = <[openehr::125]>
														list = <
															["1"] = <
																units = <"mm[Hg]">
																magnitude = <|0.0..20000.0|>
																precision = <|2|>
															>
														>
													>
												}
											}
											ELEMENT[at0090] occurrences matches {0..1} matches {    -- Reference to central corneal thickness used for correction
												value matches {
													DV_URI matches {*}
												}
											}
											ELEMENT[at0082] occurrences matches {0..1} matches {    -- Type of correction
												value matches {
													DV_TEXT matches {*}
													DV_CODED_TEXT matches {
														defining_code matches {
															[local::
															at0097,    -- IOPcc - corneal-compensated ocular response analysis
															at0098]    -- bIOP - biomechanically corrected noncontact IOP
														}
													}
												}
											}
										}
									}
									ELEMENT[at0065] occurrences matches {0..1} matches {    -- Time to first applanation
										value matches {
											C_DV_QUANTITY <
												property = <[openehr::128]>
												list = <
													["1"] = <
														units = <"ms">
														magnitude = <|>=0.0|>
														precision = <|0|>
													>
												>
											>
										}
									}
									ELEMENT[at0093] occurrences matches {0..1} matches {    -- Ocular response analysis waveform score
										value matches {
											DV_QUANTITY matches {*}
										}
									}
									ELEMENT[at0061] occurrences matches {0..*} matches {    -- Clinical interpretation
										value matches {
											DV_TEXT matches {*}
										}
									}
									allow_archetype CLUSTER[at0080] occurrences matches {0..*} matches {    -- Multimedia
										include
											archetype_id/value matches {/openEHR-EHR-CLUSTER\.multimedia(-[a-zA-Z0-9_]+)*\.v1/}
									}
									ELEMENT[at0063] occurrences matches {0..1} matches {    -- Comment
										value matches {
											DV_TEXT matches {*}
										}
									}
									allow_archetype CLUSTER[at0078] occurrences matches {0..*} matches {    -- Test not done
										include
											archetype_id/value matches {/openEHR-EHR-CLUSTER\.exclusion_exam\.v1/}
									}
								}
							}
						}
						state matches {
							ITEM_TREE[at0007] matches {    -- Tree
								items cardinality matches {0..*; unordered} matches {
									ELEMENT[at0046] occurrences matches {0..1} matches {    -- Method type
										value matches {
											DV_CODED_TEXT matches {
												defining_code matches {
													[local::
													at0047,    -- Goldmann
													at0048,    -- Perkins
													at0049,    -- Tono-Pen
													at0050,    -- Rebound
													at0051,    -- Dynamic Contour
													at0053,    -- Transpalpebral tonometer
													at0066,    -- Non-contact tonometry
													at0083,    -- Implanted pressure sensor
													at0084,    -- IOPg - Goldmann-Corellated Ocular Reponse Analysis
													at0085,    -- Palpation
													at0086,    -- Pneumatotonometer
													at0087]    -- Schiøtz
												}
											}
										}
									}
									allow_archetype CLUSTER[at0055] occurrences matches {0..1} matches {    -- Device details
										include
											archetype_id/value matches {/openEHR-EHR-CLUSTER\.device(-[a-zA-Z0-9_]+)*\.v1/}
									}
									ELEMENT[at0074] occurrences matches {0..*} matches {    -- Confounding factors
										value matches {
											DV_TEXT matches {*}
											DV_CODED_TEXT matches {
												defining_code matches {
													[local::
													at0096,    -- Mydriasis
													at0132,    -- During general anaesthesia or sedation, presumption of significant effect on pressure
													at0133]    -- During general anaesthesia or sedation, presumption of minor effect on pressure.
												}
											}
										}
									}
									ELEMENT[at0095] occurrences matches {0..1} matches {    -- Prism graduation mark on 43° offset mark
										value matches {
											C_DV_QUANTITY <
												property = <[openehr::497]>
												list = <
													["1"] = <
														units = <"deg">
													>
												>
											>
										}
									}
									ELEMENT[at0094] occurrences matches {0..1} matches {    -- Prism graduation mark on horizontal meridian
										value matches {
											C_DV_QUANTITY <
												property = <[openehr::497]>
												list = <
													["1"] = <
														units = <"deg">
													>
												>
											>
										}
									}
									allow_archetype CLUSTER[at0079] occurrences matches {0..*} matches {    -- Extension
										include
											archetype_id/value matches {/.*/}
									}
									ELEMENT[at0105] occurrences matches {0..1} matches {    -- Position
										value matches {
											DV_CODED_TEXT matches {
												defining_code matches {
													[local::
													at0106,    -- Standing
													at0107,    -- Sitting
													at0108,    -- Lying in supine position
													at0109,    -- Lying in prone position
													at0110,    -- Lying in left lateral decubitus position
													at0111]    -- Lying in right lateral decubitus position
												}
											}
										}
									}
									ELEMENT[at0113] occurrences matches {0..1} matches {    -- Current number of glaucoma medications
										value matches {
											DV_COUNT matches {
												magnitude matches {|0..10|}
											}
										}
									}
									CLUSTER[at0116] occurrences matches {0..*} matches {    -- Relationship to Medication
										items cardinality matches {1..*; unordered} matches {
											ELEMENT[at0117] occurrences matches {0..1} matches {    -- Link to resource describing medication use
												value matches {
													DV_EHR_URI matches {*}
												}
											}
											ELEMENT[at0118] occurrences matches {0..1} matches {    -- Type of relationship
												value matches {
													DV_CODED_TEXT matches {
														defining_code matches {
															[local::
															at0119,    -- Long-term medication
															at0120,    -- Acute Intervention
															at0121,    -- Trial of medication
															at0122]    -- Provocation test
														}
													}
												}
											}
											ELEMENT[at0124] occurrences matches {0..1} matches {    -- Duration between last administration and pressure measurement
												value matches {
													DV_DURATION matches {*}
												}
											}
										}
									}
									CLUSTER[at0114] occurrences matches {0..*} matches {    -- Related procedure
										items cardinality matches {1..*; unordered} matches {
											ELEMENT[at0125] occurrences matches {0..*} matches {    -- Related procedure name
												value matches {
													DV_CODED_TEXT matches {
														defining_code matches {
															[local::
															at0134,    -- Suture removal
															at0135,    -- Ocular massage
															at0136,    -- Laser suture lysis
															at0137,    -- Intracameral Injection
															at0138,    -- Cameral Paracenthesis
															at0139,    -- Corneal wound burp
															at0140,    -- Intraviteal Injection
															at0141,    -- Water drinking test
															at0142,    -- Laser Iridotomy
															at0143]    -- Laser vitreolysis
														}
													}
												}
											}
											ELEMENT[at0115] occurrences matches {0..1} matches {    -- Link to procedure
												value matches {
													DV_EHR_URI matches {*}
												}
											}
											ELEMENT[at0131] occurrences matches {0..1} matches {    -- Temporal relationship
												value matches {
													DV_DURATION matches {*}
													DV_CODED_TEXT matches {
														defining_code matches {
															[local::
															at0144,    -- Before procedure
															at0145,    -- During procedure
															at0146]    -- After procedure
														}
													}
												}
											}
										}
									}
								}
							}
						}
					}
				}
			}
		}
		protocol matches {
			ITEM_TREE[at0068] matches {*}    -- Tree
		}
	}

