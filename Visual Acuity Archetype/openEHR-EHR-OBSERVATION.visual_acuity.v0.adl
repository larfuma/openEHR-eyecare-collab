definition
	OBSERVATION[at0000] matches {    -- Visual acuity test result
		data matches {
			HISTORY[at0001] matches {    -- Event Series
				events cardinality matches {1..*; unordered} matches {
					EVENT[at0134] occurrences matches {0..*} matches {    -- Any event
						data matches {
							ITEM_TREE[at0003] matches {    -- Tree
								items cardinality matches {0..*; unordered} matches {
									ELEMENT[at0007] matches {    -- Eye(s) examined
										value matches {
											DV_CODED_TEXT matches {
												defining_code matches {
													[local::
													at0012,    -- Left eye
													at0013,    -- Right eye
													at0014]    -- Both eyes simultaneously
												}
											}
										}
									}
									CLUSTER[at0053] occurrences matches {0..1} matches {      -- Result details
										items cardinality matches {1..*; unordered} matches {
											ELEMENT[at0151] occurrences matches {0..1} matches {    -- Result
												value matches {
													DV_CODED_TEXT matches {
														defining_code matches {
															[local::
															at0152,    -- HM - Hand movement
															at0153,    -- CF - Count fingers
															at0154,    -- PL - Perceives Light
															at0155,    -- PLAP - Perceives Light, accurate Projection
															at0156,    -- PLIP - Perceives Light, inaccurate Projection
															at0157,    -- CSM - Central Steady Maintained Fixation
															at0158,    -- CUM - Central Unsteady Maintained Fixation
															at0159,    -- FF - Fix and Follow
															at0160,    -- BFL - Blinks for Light
															at0161,    -- NPL - No Light Perception
															at0260,    -- OtO - Objection to Occlusion
															at0280]    -- NI - No Improvement
														}
													}
													DV_QUANTITY matches {*}
													DV_COUNT matches {*}
													DV_PROPORTION matches {
														type matches {3}
													}
												}
											}
											ELEMENT[at0164] occurrences matches {0..1} matches {    -- Unit of Result
												value matches {
													DV_CODED_TEXT matches {
														defining_code matches {
															[local::
															at0165,    -- logMAR
															at0166,    -- MAR
															at0167,    -- Number of Optotypes Identified
															at0168,    -- Decimal
															at0169,    -- VAS
															at0170,    -- Jaeger J
															at0171,    -- Meter Snellen Ratio
															at0172,    -- Feet Snellen Ratio
															at0281]    -- N Font Size
														}
													}
												}
											}
											CLUSTER[at0193] occurrences matches {0..1} matches {    -- Derived Result
												items cardinality matches {1..*; unordered} matches {
													ELEMENT[at0173] occurrences matches {0..1} matches {    -- Derived Result
														value matches {
															DV_CODED_TEXT matches {
																defining_code matches {
																	[local::
																	at0174,    -- HM - Hand movement
																	at0175,    -- CF - Count fingers
																	at0176,    -- PL - Perceives Light
																	at0177,    -- Perceives Light - accurate Projection
																	at0178,    -- Perceives Light - inaccurate Projection
																	at0179,    -- CSM - Central Steady Maintained Fixation
																	at0180,    -- CUM - Central Unsteady Maintained Fixation
																	at0181,    -- FF - Fix and Follow
																	at0182,    -- BFL - Blinks for Light
																	at0183]    -- NPL - No Light Perception
																}
															}
															DV_QUANTITY matches {*}
															DV_COUNT matches {*}
															DV_PROPORTION matches {*}
														}
													}
													ELEMENT[at0282] occurrences matches {0..1} matches {    -- Unit of derived Result
														value matches {
															DV_CODED_TEXT matches {
																defining_code matches {
																	[local::
																	at0283,    -- logMAR
																	at0284,    -- MAR
																	at0285,    -- Number of Optotypes Identified
																	at0286,    -- Decimal
																	at0287,    -- VAS
																	at0288,    -- Jaeger J
																	at0289,    -- Meter Snellen Ratio
																	at0290,    -- Feet Snellen Ratio
																	at0291]    -- N Font Size
																}
															}
														}
													}
												}
											}
											ELEMENT[at0055] occurrences matches {0..1} matches {    -- No test result
												value matches {
													DV_BOOLEAN matches {
														value matches {true}
													}
												}
											}
											ELEMENT[at0140] occurrences matches {0..1} matches {    -- Reason for no test result
												value matches {
													DV_TEXT matches {*}
												}
											}
										}
									}
									ELEMENT[at0040] occurrences matches {0..1} matches {    -- Comment
										value matches {
											DV_TEXT matches {*}
										}
									}
									ELEMENT[at0258] occurrences matches {0..1} matches {    -- BCVA Tag
										value matches {
											DV_CODED_TEXT matches {
												defining_code matches {
													[local::at0259]    -- BCVA
												}
											}
										}
									}
									allow_archetype CLUSTER[at0141] occurrences matches {0..*} matches {    -- Test not done
										include
