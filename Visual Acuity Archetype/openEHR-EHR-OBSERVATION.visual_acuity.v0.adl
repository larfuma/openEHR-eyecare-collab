					text = <"Fogging lens">
					description = <"*A Lens used to intentionally blur the vision in that eye. (en)">
				>
				["at0384"] = <
					text = <"Pinhole Occluder">
					description = <"*A pinhole occluder was used which is a device with small holes that act as apertures. Sometimes used together with lenses. (en)">
				>
				["at0385"] = <
					text = <"*Subjective Overrefraction-based Lenses (en)">
					description = <"*Lenses with values based on subjective overrefraction. Typically used together with the corrective device that was present during overrefraction. (en)">
				>
				["at0386"] = <
					text = <"None - Uncorrected">
