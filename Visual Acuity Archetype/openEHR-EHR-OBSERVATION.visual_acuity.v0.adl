								items cardinality matches {0..*; unordered} matches {
									ELEMENT[at0112] occurrences matches {0..*} matches {    -- Patient circumstances affecting result
										value matches {
											DV_TEXT matches {*}
											DV_CODED_TEXT matches {
												defining_code matches {
													[local::
													at0195,    -- Mydriasis
													at0196,    -- Lid Swelling
													at0197,    -- Blepharospasm
													at0198,    -- Confusion
													at0199,    -- Excentric Fixation
